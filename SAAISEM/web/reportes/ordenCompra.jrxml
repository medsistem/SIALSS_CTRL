	<parameter name="ordenCompra" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
	w.F_DesProy,
	o.F_NoCompra,
	p.F_NomPro,
  DATE_FORMAT(o.F_FecSur, '%d/%m/%Y'),
	o.F_HorSur,
	uc.F_Usu as F_Usu,
	F_StsPed,
	o.F_FecSur,
	o.F_Clave,
	o.F_Obser,
	m.F_DesPro,
	o.F_Cant,
	( @csum := @csum + o.F_Cant ) AS totales,
	o.F_Iva,
	o.F_Precio,
	o.F_Descuento,
	( o.F_Precio * ( o.F_Descuento / 100 ) ) AS descuentoProd,
		( o.F_Cant * o.F_Precio ) AS importe,
		( @ssum := @ssum + ( o.F_Cant * o.F_Precio ) ) AS subtotal,
	( @dsum := @dsum + o.F_Cant * ( o.F_Precio * ( o.F_Descuento / 100 ) ) ) AS descuento,
		( @tsum := ( @ssum - @dsum ) + @isum ) AS total,
	(@isum := (( o.F_Cant * ( o.F_Precio - ( o.F_Precio * ( o.F_Descuento / 100 ) ) ) ) * ( o.F_Iva / 100 ) ) ) AS iva
FROM
	( SELECT @csum := 0 ) r,
	( SELECT @ssum := 0 ) s,
	( SELECT @dsum := 0 ) d,
	( SELECT @tsum := 0 ) t,
	( SELECT @isum := 0 ) i,
	tb_pedido_sialss o
	INNER JOIN tb_proveedor p	ON o.F_Provee = p.F_ClaProve
	INNER JOIN tb_medica m ON o.F_Clave = m.F_ClaPro
	INNER JOIN tb_proyectos w ON o.F_Proyecto = w.F_Id
	LEFT JOIN tb_usuariocompra uc ON uc.F_IdUsu = o.F_IdUsu

WHERE
	F_NoCompra = $P{ordenCompra} order by totales]]>
	</queryString>
